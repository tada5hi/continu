{
    "name": "continu",
    "version": "1.3.1",
    "description": "Zero dependency container/config library.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "rollup -c",
        "build": "rimraf dist && npm run build:types && npm run build:js",
        "commit": "npx git-cz",
        "lint": "eslint --ext .js,.ts ./src",
        "lint:fix": "npm run lint -- --fix",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "npm run test -- --coverage",
        "prepare": "npx husky install",
        "release": "npm run build && changelogen --release --push && npm publish"
    },
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "keywords": [
        "config",
        "config-container",
        "container",
        "option",
        "options",
        "validate",
        "validation",
        "transform",
        "transformation",
        "nested-object",
        "key-path"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tada5hi/continu.git"
    },
    "bugs": {
        "url": "https://github.com/Tada5hi/continu/issues"
    },
    "homepage": "https://github.com/Tada5hi/continu#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@swc/core": "^1.3.101",
        "@swc/jest": "^0.2.29",
        "@tada5hi/commitlint-config": "^1.1.4",
        "@tada5hi/eslint-config-typescript": "^1.2.7",
        "@tada5hi/semantic-release": "^0.3.0",
        "@tada5hi/tsconfig": "^0.5.0",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "rollup": "^4.12.1",
        "semantic-release": "^22.0.12",
        "typescript": "^5.3.3"
    }
}
