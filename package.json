{
  "name": "continu",
  "version": "1.3.2",
  "description": "Zero dependency container/config library.",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "rimraf dist && npm run build:types && npm run build:js",
    "commit": "npx git-cz",
    "lint": "eslint --ext .js,.ts ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "npx husky install",
    "release": "npm run build && changelogen --release --push && npm publish"
  },
  "author": {
    "name": "Peter Placzek",
    "email": "contact@tada5hi.net",
    "url": "https://github.com/tada5hi"
  },
  "license": "MIT",
  "keywords": [
    "config",
    "config-container",
    "container",
    "option",
    "options",
    "validate",
    "validation",
    "transform",
    "transformation",
    "nested-object",
    "key-path"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tada5hi/continu.git"
  },
  "bugs": {
    "url": "https://github.com/Tada5hi/continu/issues"
  },
  "homepage": "https://github.com/Tada5hi/continu#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@swc/core": "^1.3.67",
    "@swc/jest": "^0.2.26",
    "@tada5hi/commitlint-config": "^1.0.3",
    "@tada5hi/eslint-config-typescript": "^1.1.10",
    "@tada5hi/semantic-release": "^0.1.0",
    "@tada5hi/tsconfig": "^0.4.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "rollup": "^3.25.3",
    "semantic-release": "^21.0.6",
    "typescript": "^5.1.6"
  }
}
